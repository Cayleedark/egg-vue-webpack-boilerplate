{
  "eslint.enable": true,
  "eslint.run": "onSave",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": false
  },
  "eslint.validate": [
    "javascript",
    "vue-html",
    {
      "language": "vue",
      "autoFix": true
    }
  ],
  "files.exclude": {
    "**/node_modules/_**": true
  }
}